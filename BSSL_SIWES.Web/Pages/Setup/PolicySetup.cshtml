@page
@model BSSL_SIWES.Web.PolicySetupModel

@{
    ViewData["Title"] = "PolicySetup";
}


        <div class="content-wrapper col-lg-12" style=" margin-left: 10%">

            <div class="row">
                <div class="col-md-4">
                    <form method="post">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <label asp-for="PolicyTb.InstypeId" class="control-label"></label>
                            <input asp-for="PolicyTb.InstypeId" class="form-control" />
                            <span asp-validation-for="PolicyTb.InstypeId" class="text-danger"></span>
                        </div>
                        <div class="form-group form-check">
                            <label class="form-check-label">
                                <input class="form-check-input" asp-for="PolicyTb.MustNotExcCap" /> @Html.DisplayNameFor(model => model.PolicyTb.MustNotExcCap))
                            </label>
                        </div>
                        <div class="form-group">
                            <label asp-for="PolicyTb.MaxSiwesCap" class="control-label"></label>
                            <input asp-for="PolicyTb.MaxSiwesCap" class="form-control" />
                            <span asp-validation-for="PolicyTb.MaxSiwesCap" class="text-danger"></span>
                        </div>
                        <div class="form-group form-check">
                            <label class="form-check-label">
                                <input class="form-check-input" asp-for="PolicyTb.AllowDisporal" /> @Html.DisplayNameFor(model => model.PolicyTb.AllowDisporal))
                            </label>
                        </div>
                        <div class="form-group">
                            <label asp-for="PolicyTb.SuperAmt" class="control-label"></label>
                            <input asp-for="PolicyTb.SuperAmt" class="form-control" />
                            <span asp-validation-for="PolicyTb.SuperAmt" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="PolicyTb.StudentAmt" class="control-label"></label>
                            <input asp-for="PolicyTb.StudentAmt" class="form-control" />
                            <span asp-validation-for="PolicyTb.StudentAmt" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="PolicyTb.MinVisit4Super" class="control-label"></label>
                            <input asp-for="PolicyTb.MinVisit4Super" class="form-control" />
                            <span asp-validation-for="PolicyTb.MinVisit4Super" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="PolicyTb.MasterListDate" class="control-label"></label>
                            <input asp-for="PolicyTb.MasterListDate" class="form-control" />
                            <span asp-validation-for="PolicyTb.MasterListDate" class="text-danger"></span>
                        </div>
                        <div class="form-group form-check">
                            <label class="form-check-label">
                                <input class="form-check-input" asp-for="PolicyTb.DelistInst" /> @Html.DisplayNameFor(model => model.PolicyTb.DelistInst))
                            </label>
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Create" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
            </div>
        </div>

        @section Scripts {
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }
